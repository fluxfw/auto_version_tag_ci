{
  "name": "srag/auto_version_tag_ci",
  "description": "Auto create version tag gitlab ci on merge `develop` to `master`, among other things",
  "version": "1.8.0",
  "type": "project",
  "keywords": [
    "auto",
    "changelog",
    "ci",
    "composer",
    "description",
    "docker",
    "github",
    "gitlab",
    "homepage",
    "json",
    "keywords",
    "maintainer",
    "npm",
    "pull",
    "request",
    "short",
    "tag",
    "topics",
    "update",
    "version"
  ],
  "homepage": "https://github.com/fluxfw/auto_version_tag_ci",
  "license": "GPL-3.0-only",
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "build": [
      "php src/build.php"
    ]
  }
}