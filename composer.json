{
  "name": "srag/auto_version_tag_ci",
  "description": "Auto create version tag gitlab ci on merge `develop` to `master`, among other things",
  "version": "1.8.0",
  "extra": {
    "generate_plugin_readme_template": "LIBRARY",
    "long_description_template": "src/LONG_DESCRIPTION.md"
  },
  "type": "project",
  "keywords": [
    "auto",
    "changelog",
    "ci",
    "composer",
    "description",
    "docker",
    "github",
    "gitlab",
    "homepage",
    "json",
    "keywords",
    "maintainer",
    "npm",
    "pull",
    "request",
    "short",
    "tag",
    "topics",
    "update",
    "version"
  ],
  "homepage": "https://github.com/fluxfw/auto_version_tag_ci",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "srag/generateplugininfoshelper": ">=0.1.0"
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "build": [
      "php src/build.php"
    ],
    "generate-plugin-readme": [
      "srag\\GeneratePluginInfosHelper\\GeneratePluginReadme::generatePluginReadme"
    ]
  }
}